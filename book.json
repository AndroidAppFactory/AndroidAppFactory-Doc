  "language": "zh-hans",
  "styles": {
    "website": "http://cdn.bihe0832.com/css/global.css"
  },
  "plugins": [
    "favicon",
    "insert-logo",
    "-lunr",
    "-search",
    "search-pro",
    "expandable-chapters",
    "chapter-fold",
    "splitter",
    "hide-element",
    "pageview-count",
    "-sharing",
    "sharing-plus",
    "github",
    "github-buttons@2.1.0",
    "code",
    "popup",
    "anchor-navigation-ex",
    "prism",
    "-highlight",
    "flexible-alerts",
    "tbfed-pagefooter",
    "donate"
